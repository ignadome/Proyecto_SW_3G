<h3> BEYOND THE SCREEN</h3>
<ul>
    <li><h3><a href="/games/listajuegos">Lista de Juegos</a></h3></li>
</ul>

<div class='contenedor-index'>
    <% gameList.forEach(game => { %>
    <div class="bloque-index">
        <div class = "game_title"> <h4><a href="/games/<%= game.id %>"><%= game.title %></a></h4> </div>
        <p><a href="/games/<%= game.id %>"><img src="<%= game.image %>" alt="Imagen de <%= game.title %>"></a></p>
    </div>
    <% }); %>
</div>
